{
  "name": "esportspools/sphinxsearch",
  "description": "Laravel package to query Sphinxsearch in Laravel 5.5 and above",
  "keywords": [
    "sphinx",
    "sphinxsearch",
    "laravel",
    "laravel 5"
  ],
  "homepage": "http://github.com/esportspools/sphinxsearch",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "gsaqui",
      "email": "glenn@esportspools.com"
    },
    {
      "name": "JeremyHutchings",
      "email": "jeremy@esportspools.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "~5.5",
    "gigablah/sphinxphp": "2.0.8"
  },
  "autoload": {
    "psr-0": {
      "esportspools\\SphinxSearch": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "dev-master"
    },
    "laravel": {
      "providers": [
        "esportspools\\SphinxSerach\\SphinxSearchServiceProvider"
      ]
    }
  }
}